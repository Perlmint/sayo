    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "config": "^1.21.0",
    "css-loader": "^0.25.0",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "google-auth-library": "^0.9.8",
