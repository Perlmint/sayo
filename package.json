{
  "name": "sayo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:server": "tsc --outDir out",
    "dev:server": "tsc --outDir out --watch",
    "build:client": "webpack",
    "gen:model": "typedsequelize",
    "start:dev": "nodemon --debug --watch out --watch tsconfig.json --watch package.json --ignore out/assets out/server.js",
    "dev:client": "webpack-dev-server -d --config ./webpack.dev.config.js --watch --progress",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run start:dev\" \"npm run dev:client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perlmint/sayo.git"
